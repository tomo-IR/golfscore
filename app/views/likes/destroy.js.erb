change_button.setAttribute('href', '/likes?message_id=' + '<%= params[:message_id] %>' + '&score_id=' + '<%= params[:score_id] %>'  + '&user_id=' + '<%= params[:user_id] %>' );

document.getElementById("heart-" + "<%= params[:message_id] %>").innerText = "♡";
