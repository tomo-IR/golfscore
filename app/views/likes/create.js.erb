var like_number = document.getElementById("unlike-" + "<%= params[:message_id] %>").textContent;
document.getElementById("unlike-" + "<%= params[:message_id] %>").textContent =  Number(like_number) + 1;

var count = document.getElementById("unlike-" + "<%= params[:message_id] %>");
count.id = "like-" + "<%= params[:message_id] %>";

document.getElementById("change-like-" + "<%= params[:message_id] %>").innerText = "❤️";
var change_button = document.getElementById("change-like-" + "<%= params[:message_id] %>");
change_button.id = "change-unlike-" + "<%= params[:message_id] %>";
change_button.setAttribute('data-method', 'delete');
change_button.setAttribute('href', '/likes/' + '*?message_id=' + '<%= params[:message_id] %>' + '&user_id=' + '<%= params[:user_id] %>' );

